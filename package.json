{
  "name": "embed-table",
  "version": "4.0.5",
  "description": "Easily make nice looking text tables for Discord's embed fields and descriptions.",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TreeFarmer/embed-table.git"
  },
  "author": {
    "email": "contact@luyx.dev",
    "name": "Luyx LLC",
    "url": "https://luyx.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TreeFarmer/embed-table/issues"
  },
  "homepage": "https://embed-table.treefarmer.xyz",
  "keywords": [
    "discord",
    "discordjs",
    "messageembed",
    "table"
  ],
  "devDependencies": {
    "@types/node": "^18.11.9",
    "eslint": "^8.28.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "lint": "eslint --cache --fix"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  }
}
