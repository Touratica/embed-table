  "bugs": {
    "url": "https://github.com/TreeFarmer/embed-table/issues"
  },
  "homepage": "https://embed-table.treefarmer.xyz",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "eslint": "^8.30.0",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "directories": {
    "doc": "docs"
  }
}
